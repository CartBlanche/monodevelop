<Addin id          = "Core.Gui"
       namespace   = "MonoDevelop"
       name        = "MonoDevelop Gui Services"
       author      = "Lluis Sanchez, Todd Berman, John Luke, Mike Krueger"
       copyright   = "GPL"
       url         = "http://www.monodevelop.com"
       description = "Provides basic GUI services"
	   category    = "MonoDevelop Core"
       version     = "0.13.0"
       compatVersion = "0.13.0">

	<Runtime>
		<Import assembly="MonoDevelop.Components.dll"/>
		<Import assembly="MonoDevelop.Core.Gui.dll"/>
	</Runtime>

	<Dependencies>
		<Addin id="Core" version="0.13.0"/>
	</Dependencies>
	
	<!-- Extension points -->

	<ExtensionNodeSet id="MonoDevelop.Core.Gui.ItemSet">
		<ExtensionNode name="CommandItem" type="MonoDevelop.Core.Gui.Codons.CommandItemCodon" />
		<ExtensionNode name="SeparatorItem" type="MonoDevelop.Core.Gui.Codons.SeparatorItemCodon" />
		<ExtensionNode name="ItemSet" type="MonoDevelop.Core.Gui.Codons.ItemSetCodon" nodeset="Origin.Commands.ItemSet" >
			<ExtensionNodeSet id="MonoDevelop.Core.Gui.ItemSet" />
		</ExtensionNode>
		<ExtensionNode name="LinkItem" type="MonoDevelop.Core.Gui.Codons.LinkItemCodon" />
		<ExtensionNode name="LocalCommandItem" type="MonoDevelop.Core.Gui.Codons.LocalCommandItemCodon" />
	</ExtensionNodeSet>

	<ExtensionNodeSet id="MonoDevelop.Core.Gui.DialogPanel">
		<ExtensionNode name="DialogPanel" type="MonoDevelop.Core.Gui.Codons.DialogPanelCodon">
			<ExtensionNodeSet id="MonoDevelop.Core.Gui.DialogPanel" />
		</ExtensionNode>
	</ExtensionNodeSet>
	
	<ExtensionPoint path = "/SharpDevelop/Workbench/StockIcons" name = "Stock icons">
		<Description>Available from MonoDevelop.Core.Gui.Services.Resources.</Description>
		<ExtensionNode name="StockIcon" type="MonoDevelop.Core.Gui.Codons.StockIconCodon" />
	</ExtensionPoint>

	<!-- Extensions -->
	
	<Extension path = "/Workspace/Services">
		<Class id    = "DispatchService"
		       class = "MonoDevelop.Core.Gui.DispatchService"/>
		<Class id    = "ResourceService"
		       class = "MonoDevelop.Core.Gui.ResourceService"/>
		<Class id    = "MessageService"
		       class = "MonoDevelop.Core.Gui.MessageService"/>
	</Extension>

	<Extension path = "/System/Applications">
		<Application id    = "gsetup"
		       description = "Graphical add-in setup utility"
		       class = "MonoDevelop.Core.Gui.Dialogs.SetupApp"/>
	</Extension>

